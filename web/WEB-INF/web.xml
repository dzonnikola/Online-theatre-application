    </servlet>
    <servlet>
        <servlet-name>FilmP</servlet-name>
        <servlet-class>servleti.FilmP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registracija</servlet-name>
        <url-pattern>/Registracija</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bioskop</servlet-name>
        <url-pattern>/Bioskop</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Film</servlet-name>
        <url-pattern>/Film</url-pattern>
    </servlet-mapping>
