        <servlet-name>ServletP</servlet-name>
        <url-pattern>/ServletP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilmP</servlet-name>
        <url-pattern>/FilmP</url-pattern>
    </servlet-mapping>
