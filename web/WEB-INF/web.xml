    <servlet>
        <servlet-name>Registracija</servlet-name>
        <servlet-class>servleti.Registracija</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bioskop</servlet-name>
        <servlet-class>servleti.Bioskop</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Film</servlet-name>
        <servlet-class>servleti.Film</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sala</servlet-name>
