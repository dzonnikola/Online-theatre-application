        <servlet-name>Sala</servlet-name>
        <url-pattern>/Sala</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Projekcija</servlet-name>
        <url-pattern>/Projekcija</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rezervacija</servlet-name>
        <url-pattern>/Rezervacija</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLista</servlet-name>
        <url-pattern>/ServletLista</url-pattern>
    </servlet-mapping>
